  "Product": {
    "@context": "https://schema.org",
    "@type": "Product",
    "name": "Default Product",
    "image": "https://example.com/product.jpg",
    "description": "Default product description.",
    "brand": {
      "@type": "Brand",
      "name": "Brand Name"
    },
    "offers": {
      "@type": "Offer",
      "priceCurrency": "USD",
      "price": "99.99",
      "availability": "https://schema.org/InStock"
    }

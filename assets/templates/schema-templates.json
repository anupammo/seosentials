{
  "Article": {
    "@context": "https://schema.org",
    "@type": "Article",
    "headline": "Default Headline",
    "description": "Default article description.",
    "image": "https://example.com/default-article.jpg",
    "author": {
      "@type": "Person",
      "name": "Author Name"
    },
    "publisher": {
      "@type": "Organization",
      "name": "Your Brand",
      "logo": {
        "@type": "ImageObject",
        "url": "https://example.com/logo.png"
      }
    },
    "mainEntityOfPage": "https://example.com/article"
